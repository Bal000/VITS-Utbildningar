      providerName="System.Data.SqlClient" />
    <add name="DATABASEVITSConnectionString" connectionString="Data Source=.;AttachDbFilename=C:\Users\Felix\Documents\GitHub\VITS-Utbildningar\Vits\VitsWCF\App_Data\DATABASEVITS.mdf;Integrated Security=True;User Instance=True"
